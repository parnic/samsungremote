<UserControl x:Class="SamsungRemoteWP7.TvKeyControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="120" d:DesignWidth="90" Loaded="UserControl_Loaded"
    ManipulationStarted="UserControl_ManipulationStarted"
    ManipulationCompleted="UserControl_ManipulationCompleted"
    Tap="UserControl_Tap">
    
    <Grid x:Name="LayoutRoot">
        <Image Name="Img"  Source="Images/keypad_00.png"/>
        <TextBlock Name="Contents"  HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</UserControl>
